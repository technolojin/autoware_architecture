<?xml version="1.0"?>
<launch>
  <!-- This launch file is auto-generated by the autoware_architect -->
  <!-- Compute Unit Launcher: {{ compute_unit }} -->
  <!-- This launcher starts all namespaces assigned to compute unit: {{ compute_unit }} -->

  <!-- Current namespace -->
  <arg name="ns" description="current namespace" default=""/>

{% for namespace_info in namespaces %}
  <!-- Include {{ namespace_info.namespace }} namespace -->
  <include file="$(find-pkg-share autoware_perception_deployment)/{{ compute_unit }}/{{ namespace_info.namespace }}/{{ namespace_info.namespace }}.launch.xml">
    <!-- Namespace: {{ namespace_info.namespace }} -->
    <!-- Components: {{ namespace_info.component_count }} -->
    <arg name="ns" value="$(var ns)/{{ namespace_info.namespace }}"/>
{% for arg in namespace_info.args %}
    <arg name="{{ arg.name }}" value="{{ arg.value }}"/>
{% endfor %}
  </include>

{% endfor %}
</launch>