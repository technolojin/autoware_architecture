{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Parameters for Decorative Tracker Merger",
  "type": "object",
  "definitions": {
    "tracker_state_parameter": {
      "type": "object",
      "properties": {
        "remove_probability_threshold": {
          "type": "number",
          "description": "Threshold for removing objects based on existence probability",
          "default": 0.3,
          "minimum": 0.0,
          "maximum": 1.0
        },
        "publish_probability_threshold": {
          "type": "number",
          "description": "Threshold for publishing objects based on existence probability",
          "default": 0.5,
          "minimum": 0.0,
          "maximum": 1.0
        },
        "default_lidar_existence_probability": {
          "type": "number",
          "description": "Default existence probability for lidar objects",
          "default": 0.7,
          "minimum": 0.0,
          "maximum": 1.0
        },
        "default_radar_existence_probability": {
          "type": "number",
          "description": "Default existence probability for radar objects",
          "default": 0.6,
          "minimum": 0.0,
          "maximum": 1.0
        },
        "default_camera_existence_probability": {
          "type": "number",
          "description": "Default existence probability for camera objects",
          "default": 0.5,
          "minimum": 0.0,
          "maximum": 1.0
        },
        "decay_rate": {
          "type": "number",
          "description": "Decay rate for existence probability over time",
          "default": 0.1,
          "minimum": 0.0,
          "maximum": 1.0
        },
        "max_dt": {
          "type": "number",
          "description": "Maximum time delta for probability decay",
          "default": 1.0,
          "minimum": 0.0
        }
      },
      "required": [
        "remove_probability_threshold",
        "publish_probability_threshold",
        "default_lidar_existence_probability",
        "default_radar_existence_probability",
        "default_camera_existence_probability",
        "decay_rate",
        "max_dt"
      ],
      "additionalProperties": false
    },
    "decorative_tracker_merger": {
      "type": "object",
      "properties": {
        "base_link_frame_id": {
          "type": "string",
          "description": "Base link frame ID",
          "default": "base_link"
        },
        "merge_frame_id": {
          "type": "string",
          "description": "Frame ID for merging",
          "default": "map"
        },
        "time_sync_threshold": {
          "type": "number",
          "description": "Time synchronization threshold",
          "default": 0.999,
          "minimum": 0.0,
          "maximum": 1.0
        },
        "sub_object_timeout_sec": {
          "type": "number",
          "description": "Timeout for sub-object in seconds",
          "default": 0.8,
          "minimum": 0.0
        },
        "publish_interpolated_sub_objects": {
          "type": "boolean",
          "description": "Whether to publish interpolated sub-objects for debug",
          "default": true
        },
        "main_sensor_type": {
          "type": "string",
          "description": "Main sensor type (lidar, radar, camera)",
          "enum": ["lidar", "radar", "camera"],
          "default": "lidar"
        },
        "sub_sensor_type": {
          "type": "string",
          "description": "Sub-sensor type (lidar, radar, camera)",
          "enum": ["lidar", "radar", "camera"],
          "default": "radar"
        },
        "tracker_state_parameter": {
          "$ref": "#/definitions/tracker_state_parameter"
        },
        "enable_logging": {
          "type": "boolean",
          "description": "Whether to enable logging",
          "default": false
        },
        "logging_file_path": {
          "type": "string",
          "description": "Path to the logging file",
          "default": "association_log.json"
        },
        "delay_main_objects_tolerance": {
          "type": "number",
          "description": "Tolerance for delay in main objects (seconds)",
          "default": 1.0,
          "minimum": 0.0
        },
        "duration_empty_main_objects_tolerance": {
          "type": "number",
          "description": "Tolerance for duration of empty main objects (seconds)",
          "default": 5.0,
          "minimum": 0.0
        },
        "delay_sub_objects_tolerance": {
          "type": "number",
          "description": "Tolerance for delay in sub-objects (seconds)",
          "default": 5.0,
          "minimum": 0.0
        }
      },
      "required": [
        "base_link_frame_id",
        "merge_frame_id",
        "time_sync_threshold",
        "sub_object_timeout_sec",
        "publish_interpolated_sub_objects",
        "main_sensor_type",
        "sub_sensor_type",
        "tracker_state_parameter",
        "enable_logging",
        "logging_file_path",
        "delay_main_objects_tolerance",
        "duration_empty_main_objects_tolerance",
        "delay_sub_objects_tolerance"
      ],
      "additionalProperties": false
    }
  },
  "properties": {
    "/**": {
      "type": "object",
      "properties": {
        "ros__parameters": {
          "$ref": "#/definitions/decorative_tracker_merger"
        }
      },
      "required": ["ros__parameters"],
      "additionalProperties": false
    }
  },
  "required": ["/**"],
  "additionalProperties": false
}
